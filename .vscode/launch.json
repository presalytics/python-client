    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for API Events",
            "type": "python",
            "request": "launch",
            "module": "presalytics",
            "args": [
                "events",
                "--forward-to",
                "http://localhost:7080/"
            ],
            "justMyCode": false
        },
        {
            "name": "Client Codegen",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/sub_packages/generate_subpackages.py"
        }
    ]
}